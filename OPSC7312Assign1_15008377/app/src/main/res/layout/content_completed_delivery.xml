<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_completed_delivery"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="a15008377.opsc7312assign1_15008377.CompletedDeliveryActivity"
    tools:showIn="@layout/app_bar_completed_delivery">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_completed_delivery_report_heading"
        android:text="@string/text_completed_delivery_heading"
        style="@style/HeadingTextStyle"/>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/list_view_completed_deliveries"
        android:layout_below="@+id/text_completed_delivery_report_heading"
        android:fadeScrollbars="false"/>
</RelativeLayout>
